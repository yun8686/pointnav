extends layout
block header
  div.s-padding.row
    p.col.s12.center
      a.btn-floating.btn-large.waves-effect.waves-light.blue

block main
  ul
    - for(var　i　=　0;　i<30;i++)
      li.card.blue-grey.lighten-4
        div.card-content.row
          img.circle.card-image.col.s4(src="https://www.lawson.co.jp/ponta/tameru/bonus/img/bp_181101_122.png")
          .col.s8
            span.title| Title
            p| First Line

block footer
  .container
    .row
      .col.l6.s12
        h5.white-text| Footer Content
        p.grey-text.text-lighten-4| You can use rows and columns here to organize your footer content.
      .col.l4.offset-l2.s12
        h5.white-text| Links
        ul
          li
            a.grey-text.text-lighten-3| Link 1
            a.grey-text.text-lighten-3| Link 2
            a.grey-text.text-lighten-3| Link 3
            a.grey-text.text-lighten-3| Link 4
  .footer-copyright
    .container| © 2014 Copyright Text
    a.grey-text.text-lighten-4.right| More Links
